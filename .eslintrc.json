{
  "extends": "airbnb",
  "rules": {
    "func-names": "off",
    "no-plusplus": "off",
    "eqeqeq": "off",
    "arrow-parens": "off",
    "comma-dangle": "off",
    "no-else-return": "off",
    "no-underscore-dangle": "off"
  },
  "ignore": [
    "node_modules/**/*.*"
  ],
  "env": {
    "node": true,
    "browser": true
  }
}
